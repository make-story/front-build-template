{
  "name": "front-build.git",
  "version": "1.0.0",
  "description": "",
  "main": "servers/main.js",
  "scripts": {
    "help": "echo \"개빌모드(watch) :\n$ yarn wabpack-watch\n$ yarn server-watch\"",
    "help-npm-run-all": "echo \"package.json 내부 scripts 여러 명령 동시 실행\"",
    "lint": "eslint src --fix",
    "local": "source ./env/local.env; npm-run-all build --parallel server",
    "test": "source ./env/test.env; npm-run-all build --parallel server",
    "stage": "source ./env/stage.env; npm-run-all build --parallel server",
    "production": "source ./env/production.env; npm-run-all build --parallel server",
    "webpack-build": "webpack --config ./webpack.config.js --mode production",
    "webpack-node": "webpack --config ./config/webpack.node.js --mode production",
    "webpack-watch": "webpack --config ./webpack.config.js --watch",
    "webpack-dev-server": "webpack-dev-server --config webpack.config.js --progress --mode development",
    "webpack-react": "webpack --config ./webpack.config.js --mode production --project react",
    "webpack-react-watch": "webpack --config ./webpack.config.js --watch --project react",
    "webpack-react-dev-server": "webpack-dev-server --config webpack.config.js --progress --mode development --project react",
    "webpack-typescript": "webpack --config ./webpack.config.js --mode production --project typescript",
    "webpack-typescript-watch": "webpack --config ./webpack.config.js --watch --project typescript",
    "webpack-typescript-dev-server": "webpack-dev-server --config webpack.config.js --progress --mode development --project typescript",
    "webpack-react-typescript": "webpack --config ./webpack.config.js --mode production --project react-typescript",
    "webpack-template": "webpack --config ./webpack.config.js --mode none --project template",
    "server": "node servers/main.js",
    "server-monit": "./node_modules/pm2/bin/pm2 monit",
    "server-show": "./node_modules/pm2/bin/pm2 show main",
    "server-log": "./node_modules/pm2/bin/pm2 log",
    "server-test": "./node_modules/pm2/bin/pm2 start ./servers/main.js --watch --ignore-watch='dist data' --no-daemon",
    "server-watch": "./node_modules/pm2/bin/pm2 start ./servers/main.js --watch dist &&  && ./node_modules/pm2/bin/pm2 log",
    "server-start": "./node_modules/pm2/bin/pm2 start ./servers/main.js",
    "server-stop": "./node_modules/pm2/bin/pm2 stop ./servers/main.js",
    "server-nodemon": "nodemon --watch ./dist --ignore ./data servers/main.js"
  },
